{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "Readefine",
    "description": "YESSSSSSSSSSSSSS",
    "permissions": ["activeTab", "scripting", "webNavigation"],

    "action": {
        "default_popup": "js/index.html",
        "default_title": "React Chrome Extension"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/contentScript.js", "js/background.js"]

        }
    ]
}