{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "Readefine",
    "description": "YESSSSSSSSSSSSSS",
    "permissions": ["activeTab", "scripting", "webNavigation", "storage"],

    "action": {
        "default_popup": "js/index.html",
        "default_title": "ReaDefine"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/foreground.js"]

        }
    ], 
    "background":{
        "service_worker": "js/background.js"
    }
}